<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <!--//登陆用的servelet-->
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>BaseFunction.login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <!--//注册用的servelet-->
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>BaseFunction.register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <!--//人脸识别用的servelet-->
    <servlet>
        <servlet-name>Photocmp</servlet-name>
        <servlet-class>FaceCompare.Photocmp</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Photocmp</servlet-name>
        <url-pattern>/Photocmp</url-pattern>
    </servlet-mapping>
    <!--//打开创建会议用的servelet-->
    <servlet>
        <servlet-name>openmeeting</servlet-name>
        <servlet-class>BaseFunction.openmeeting</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>openmeeting</servlet-name>
        <url-pattern>/openmeeting</url-pattern>
    </servlet-mapping>
    <!--//开放会议用的servelet-->
    <servlet>
        <servlet-name>holdmeeting</servlet-name>
        <servlet-class>BaseFunction.holdmeeting</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>holdmeeting</servlet-name>
        <url-pattern>/holdmeeting</url-pattern>
    </servlet-mapping>
    <!--//删除会议用的servelet-->
    <servlet>
        <servlet-name>deletemeeting</servlet-name>
        <servlet-class>BaseFunction.deletemeeting</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>deletemeeting</servlet-name>
        <url-pattern>/deletemeeting</url-pattern>
    </servlet-mapping>

</web-app>